{
    "Geometry Dash": {
        "gd profile": {
            "arguments": {
                "username": {
                    "required": true,
                    "type": "String",
                    "description": "Fetches the data of this username."
                }
            },
            "description": "Fetches a Geometry Dash player's data.",
            "example": "/gd profile username: ExampleName"
        }
    },
    "BTD6": {
        "btd6 connect":{
            "arguments": {
                "oak_key": {
                    "required": false,
                    "default": null,
                    "type": "String",
                    "description": "The Open Access Key to link your account to. If left none, it tells you how."
                }
            },
            "description": "Link your BTD6 account to the discord bot."
        },
        "btd6 racedata": {
            "arguments": {
                "race_id": {
                    "required": true,
                    "type": "String",
                    "description": "The ID of the race you want to view the data for."
                }
            },
            "description": "View data of the specified race ID in BTD6."
        },
        "btd6 racelb": {
            "arguments": {
                "race_id": {
                    "required": true,
                    "type": "String",
                    "description": "The ID of the race you want to view the data for."
                }
            },
            "description": "View the leaderboard of the specified race ID in BTD6."
        },
        "btd6 races": {
            "description": "View the current races in BTD6."
        },
        "btd6 profile": {
            "arguments": {
                "oak_key": {
                    "required": false,
                    "default": "Linked account",
